    }
   ],
   "source": [
    "x=np.linspace(-10,10,100)\n",
    "y=softmax(x)\n",
    "plt.plot(x,y)\n",
    "plt.title('softmax')\n",
    "plt.grid()\n",
    "plt.show()"
   ]
  }
